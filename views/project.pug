doctype html
html(lang="en")
  head
    meta(charset="utf-8")
    title Industro - Industrial HTML Template
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(name="keywords", content="")
    meta(name="description", content="")

    // Favicon
    link(rel="icon", href="img/favicon.ico")

    // Google Web Fonts
    link(rel="preconnect", href="https://fonts.googleapis.com")
    link(rel="preconnect", href="https://fonts.gstatic.com", crossorigin)
    link(href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;500;600&family=Rubik:wght@500;600;700&display=swap", rel="stylesheet")

    // Icon Font Stylesheet
    link(href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css", rel="stylesheet")
    link(href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css", rel="stylesheet")

    // Libraries Stylesheet
    link(href="lib/animate/animate.min.css", rel="stylesheet")
    link(href="lib/owlcarousel/assets/owl.carousel.min.css", rel="stylesheet")

    // Customized Bootstrap Stylesheet
    link(href="/css/bootstrap.min.css", rel="stylesheet")

    // Template Stylesheet
    link(href="/css/style.css", rel="stylesheet")
  body
    // Spinner Start
    #spinner.show.bg-white.position-fixed.translate-middle.w-100.vh-100.top-50.start-50.d-flex.align-items-center.justify-content-center
      .spinner-border.text-primary(role="status", style="width: 3rem; height: 3rem;")
    


    // Navbar Start
    nav.navbar.navbar-expand-lg.bg-white.navbar-light.sticky-top.py-0.pe-5
      a.navbar-brand.ps-5.me-0(href="index.html")
        h1.text-white.m-0 Industro
      button.navbar-toggler.me-0(type="button", data-bs-toggle="collapse", data-bs-target="#navbarCollapse")
        span.navbar-toggler-icon
      .collapse.navbar-collapse#navbarCollapse
        .navbar-nav.ms-auto.p-4.p-lg-0
          a.nav-item.nav-link(href="/") Hogar
          a.nav-item.nav-link(href="/proyectos") Proyectos
          a.nav-item.nav-link(href="/contacto") Contacto


    // Page Header Start
    .container-fluid.page-header.py-5.mb-5.wow.fadeIn(data-wow-delay="0.1s")
      .container.py-5
        h1.display-3.text-white.animated.slideInRight Projects
        nav.aria-label="breadcrumb"
          ol.breadcrumb.animated.slideInRight.mb-0
            li.breadcrumb-item
              a(href="#") Home
            li.breadcrumb-item
              a(href="#") Pages
            li.breadcrumb-item.active(aria-current="page") Projects

    // Project Start
    .container-fluid.py-5.my-5.px-0
      .text-center.mx-auto.wow.fadeIn(data-wow-delay="0.1s", style="max-width: 600px;")
        p.fw-medium.text-uppercase.text-primary.mb-2 Our Projects
        h1.display-5.mb-5 See What We Have Completed Recently
      .owl-carousel.project-carousel.bg-dark.wow.fadeIn(data-wow-delay="0.1s")
        a.project-item(href="")
          img.img-fluid(src="/img/project-1.jpg", alt="")
          .project-title
            h5.text-primary.mb-0 Auto Engineering
        a.project-item(href="")
          img.img-fluid(src="/img/project-2.jpg", alt="")
          .project-title
            h5.text-primary.mb-0 Civil Engineering
        a.project-item(href="")
          img.img-fluid(src="/img/project-3.jpg", alt="")
          .project-title
            h5.text-primary.mb-0 Gas Engineering
        a.project-item(href="")
          img.img-fluid(src="/img/project-4.jpg", alt="")
          .project-title
            h5.text-primary.mb-0 Power Engineering
        a.project-item(href="")
          img.img-fluid(src="/img/project-5.jpg", alt="")
          .project-title
            h5.text-primary.mb-0 Energy Engineering
        a.project-item(href="")
          img.img-fluid(src="/img/project-6.jpg", alt="")
          .project-title
            h5.text-primary.mb-0 Water Engineering

    // Footer Start
    .container-fluid.bg-dark.footer.mt-5.py-5.wow.fadeIn(data-wow-delay="0.1s")
      .container.py-5
        .row.g-5
          .col-lg-3.col-md-6
            h5.text-white.mb-4 Our Office
            p.mb-2
              i.fa.fa-map-marker-alt.me-3
              | 123 Street, New York, USA
            p.mb-2
              i.fa.fa-phone-alt.me-3
              | +012 345 67890
            p.mb-2
              i.fa.fa-envelope.me-3
              | info@example.com
            .d-flex.pt-3
              a.btn.btn-square.btn-primary.rounded-circle.me-2(href="") i.fab.fa-twitter
              a.btn.btn-square.btn-primary.rounded-circle.me-2(href="") i.fab.fa-facebook-f
              a.btn.btn-square.btn-primary.rounded-circle.me-2(href="") i.fab.fa-youtube
              a.btn.btn-square.btn-primary.rounded-circle.me-2(href="") i.fab.fa-linkedin-in
          .col-lg-3.col-md-6
            h5.text-white.mb-4 Quick Links
            a.btn.btn-link(href="") About Us
            a.btn.btn-link(href="") Contact Us
            a.btn.btn-link(href="") Our Services
            a.btn.btn-link(href="") Terms & Condition
            a.btn.btn-link(href="") Support
          .col-lg-3.col-md-6
            h5.text-white.mb-4 Business Hours
            p.mb-1 Monday - Friday
            h6.text-light 09:00 am - 07:00 pm
            p.mb-1 Saturday
            h6.text-light 09:00 am - 12:00 pm
            p.mb-1 Sunday
            h6.text-light Closed
          .col-lg-3.col-md-6
            h5.text-white.mb-4 Newsletter
            p Dolor amet sit justo amet elitr clita ipsum elitr est.
            .position-relative.w-100
              input.form-control.bg-transparent.w-100.py-3.ps-4.pe-5(type="text", placeholder="Your email")
              button.btn.btn-primary.py-2.position-absolute.top-0.end-0.mt-2.me-2 SignUp

    // Copyright Start
    .container-fluid.copyright.bg-dark.py-4
      .container.text-center
        p.mb-2 Copyright &copy;
          a.fw-semi-bold(href="#") Your Site Name
          | , All Right Reserved.
        // *** This template is free as long as you keep the footer author’s credit link/attribution link/backlink. If you'd like to use the template without the footer author’s credit link/attribution link/backlink, you can purchase the Credit Removal License from "https://htmlcodex.com/credit-removal". Thank you for your support. ***
        p.mb-0 Designed By
          a.fw-semi-bold(href="https://htmlcodex.com") HTML Codex
          |  Distributed By:
          a(href="https://themewagon.com") ThemeWagon

    // Back to Top
    a.btn.btn-lg.btn-primary.btn-lg-square.rounded-circle.back-to-top(href="#") i.bi.bi-arrow-up

    // JavaScript Libraries
    script(src="https://code.jquery.com/jquery-3.4.1.min.js")
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js")
    script(src="lib/wow/wow.min.js")
    script(src="lib/easing/easing.min.js")
    script(src="lib/waypoints/waypoints.min.js")
    script(src="lib/owlcarousel/owl.carousel.min.js")
    script(src="lib/counterup/counterup.min.js")

    // Template Javascript
    script(src="/js/main.js")
